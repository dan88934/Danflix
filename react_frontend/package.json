{
  "name": "netflix",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "firebase": "^9.0.1",
    "fuse.js": "^6.4.6",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.1",
    "react-scripts": "3.4.0",
    "styled-components": "^5.3.1"
  },
  "scripts": {
    "build": "webpack build",
    "start": "webpack-dev-server — mode development — hot — open — port 3100",
    "test": "react-scripts test --coverage --watchAll",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js",
      "!<rootDir>/src/app.js",
      "!<rootDir>/src/helpers/routes.js",
      "!<rootDir>/src/lib/firebase.js",
      "!<rootDir>/src/lib/firebase.prod.js",
      "!<rootDir>/src/index.js",
      "!<rootDir>/src/seed.js",
      "!<rootDir>/src/utils/index.js",
      "!<rootDir>/src/pages/index.js",
      "!<rootDir>/src/global-styles.js",
      "!<rootDir>/src/components/index.js",
      "!<rootDir>/src/firebase.prod.js",
      "!<rootDir>/src/hooks/*.js",
      "!<rootDir>/src/context/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "html",
      "text"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.14.5",
    "@babel/plugin-transform-react-inline-elements": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.15.4",
    "@svgr/webpack": "^4.3.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^12.1.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "css-loader": "^3.4.2",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "file-loader": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.1.1",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^2.3.4",
    "url-loader": "^2.3.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^3.10.2"
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
